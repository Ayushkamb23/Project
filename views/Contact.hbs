<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <title>e-commerce</title>
   
  </head>
  <body>
    
{{>navbar data=informaitions}}
<h1 class="check text-center mt-4"> CheckOut</h1>
<section  id="contact_section" class="py-2 mt-3">
      <div class="container">
        <h3>contact informaitions</h3>
        <div class="row">
          <div class="col-md-12">


            <form action="/contact_form" method="post">

            <div class="row">
  <div class="col">
    <div id="emailHelp" class="form-text my-3">FIRST NAME</div>
    <input name="First" type="text" class="form-control" placeholder="First name" aria-label="First name">
  </div>
  <div class="col">
      <div id="emailHelp" class="form-text my-3">LAST NAME</div>
    <input name="Last" type="text" class="form-control" placeholder="Last name" aria-label="Last name">
  </div>
</div>
    <div class="mb-3">
   <div id="emailHelp" class="form-text">PHONE NUMBER</div>
    <label for="phonenumberField" class="form-label"></label>
    <input name="Phone" type="number" class="form-control " placeholder="Phone number" id="Field" aria-describedby="UsernameHelp">
  
  </div>

  <div class="mb-3">
<div id="emailHelp" class="form-text ">EMAIL ADDRESS</div>
    <label for="exampleInputEmail1" class="form-label"></label>
    <input name="Email" type="email" class="form-control" placeholder="Your Email" id="exampleInputEmail1" aria-describedby="emailHelp">
   
  </div>
 





            </div>
        </div>
      </div>

    </section> 

<section  id="contact_section" class="py-2"></section>
      <div class="container">
        <h3>Shipping Address</h3>
        <div class="row">
          <div class="col-md-12">
 <div class="mb-3">
   <div id="emailHelp" class="form-text">STREET ADDRESS</div>
    <label for="AddressField" class="form-label"></label>
    <input name="Adress" type="text" class="form-control " placeholder="Street Address" id="Field" aria-describedby="UsernameHelp">
  
  </div>


   <div class="mb-3">
   <div id="emailHelp" class="form-text">COUNTRY</div>
    <label for="COUNTRYField" class="form-label"></label>
    <input name="Country" type="text" class="form-control " placeholder="Country" id="Field" aria-describedby="UsernameHelp">
  
  </div>


   <div class="mb-3">
   <div id="emailHelp" class="form-text">TOWN / City</div>
    <label for="TowncityField" class="form-label"></label>
    <input name="Town" type="text" class="form-control " placeholder="Town/City" id="Field" aria-describedby="UsernameHelp">
  
  </div>

<div class="row">
  <div class="col">
    <div id="emailHelp" class="form-text my-3">STATE</div>
    <input name="State" type="text" class="form-control" placeholder="State" aria-label="First name">
  </div>
  <div class="col">
      <div id="emailHelp" class="form-text my-3">ZIP CODE</div>
    <input name="Zipcode" type="text" class="form-control" placeholder="Zip Code" aria-label="Last name">
  </div>
</div>

    <div class="mb-3 form-check mt-3">
    <input  type="checkbox" class="form-check-input " id="exampleCheck1">
    <label class="form-check-label" for="exampleCheck1">Use a different billing address (optional)</label>
  </div>

            </div>
        </div>
      </div>

    </section> 


    <section  id="contact_section" class="py-2">
      <div class="container">
        <h3>Payment method</h3>
        <div class="row">
          <div class="col-md-12">
   <input name="PAY" type="text" class="form-control mt-4" placeholder="Pay by Card Credit" aria-label="Last name">
      <input name="PAYPAL" type="text" class="form-control mt-3" placeholder="Paypal" aria-label="Last name">

<hr> </hr>
 <div class="col">
      <div id="emailHelp" class="form-text my-3">CARD NUMBER</div>
    <input name="Card" type="number" class="form-control" placeholder="1234 1234 1234 " aria-label="Last name">
  </div>


    <div class="row">
  <div class="col">
    <div id="emailHelp" class="form-text my-3">EXPIRATION DATE </div>
    <input name="Expiration" type="number" class="form-control" placeholder="MM/YY" aria-label="First name">
  </div>
  <div class="col">
      <div id="emailHelp" class="form-text my-3">CVC</div>
    <input name="CVC" type="number" class="form-control" placeholder="CVC code" aria-label="Last name">
  </div>
</div>
<div class="container text-center mt-5">
   <button type="submit" class="btn btn-primary">Sign UP</button>
</div>
  </form>
              </div>
        </div>
      </div>

    </section>
   
  

    <!-- Optional JavaScript; choose one of the two! -->

    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

    <!-- Option 2: Separate Popper and Bootstrap JS -->
    <!--
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
    -->
  </body>
</html>



